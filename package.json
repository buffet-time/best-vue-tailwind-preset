{
	"name": "best-vue-preset",
	"private": false,
	"version": "1.0.0",
	"type": "module",
	"author": {
		"email": "packageJson@danielTurcich.com",
		"name": "Daniel Turcich",
		"url": "https://danielTurcich.com"
	},
	"engines": {
		"pnpm": ">=8.1.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"lint-fix": "eslint --fix \"**/*.{ts,vue}\"",
		"lint-check": "eslint \"**/*.{ts,vue}\"",
		"format-fix": "prettier --check --write \"**/**\" ",
		"format-check": "prettier --check \"**/**\"",
		"type-check": "vue-tsc --noEmit",
		"checks": "pnpm lint-check && pnpm format-check && pnpm type-check"
	},
	"dependencies": {
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.2.3",
		"typescript": "^5.1.6",
		"vite": "^4.4.5",
		"vue-tsc": "^1.8.5",
		"@nabla/vite-plugin-eslint": "^1.5.0",
		"@types/node": "^20.4.5",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"@vue/eslint-config-typescript": "^11.0.3",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-vue": "^9.16.1",
		"husky": "^8.0.3",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"tailwindcss": "^3.3.3",
		"vue-eslint-parser": "^9.3.1"
	}
}
